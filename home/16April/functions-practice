//question==4;

let example=function name(a,b) {
    return a*b;
}
let ans=example;
console.log(ans(145,566));
let ans1=example;
console.log(ans1(678,905));
let ans2=example;
console.log(ans2(897,342));
let ans3=example;
console.log(ans3(908,222));
let ans4=example;
console.log(ans4(906,343));
let ans5=example;
console.log(ans5(2267,456));
let ans6=example;
console.log(ans6(845,566));
