//FAT ARROW FUNCTION
//question==1;
console.log("FAT ARROW FUNCTION");

let space=()=>{
    console.log("hello");
}
space();

//question==2;
console.log("FAT ARROW WITH MULTI PARAMETRES");

let space1=(name,surname,address,city)=>{
    console.log("my name is",name,surname,"i live in ",address,"my city is",city);
}
console.log(space1("dikshit","negta","karol bagh","delhi"));

//question==3;
console.log("FAT ARROW WITH SINGLE PARAMETRES");

